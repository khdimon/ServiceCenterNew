<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="hibernate.dialect">org.hibernate.dialect.MySQLDialect</property>
        <property name="connection.url">jdbc:mysql://localhost:3306/service_center_new</property>
        <property name="connection.driver_class">com.mysql.jdbc.Driver</property>

        <property name="connection.username">root</property>
        <property name="connection.password">root</property>

        <mapping class="com.softserve.edu.service_center_new.entity.State"/>
        <mapping class="com.softserve.edu.service_center_new.entity.Role"/>
        <mapping class="com.softserve.edu.service_center_new.entity.Team"/>
        <mapping class="com.softserve.edu.service_center_new.entity.Service"/>
        <mapping class="com.softserve.edu.service_center_new.entity.User"/>
        <mapping class="com.softserve.edu.service_center_new.entity.Order"/>

    </session-factory>
</hibernate-configuration>